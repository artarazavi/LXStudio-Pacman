{
  /* HiRes grid for capturing TE Output */
  label: "HRGrid",
   tags: [ "projector"],
   parameters: {
     "width": { type: "int", default: "640" },
     "height": { type: "int", default: "480" },
     "spacing": { type: "float", default: "5" }
   },

  "components": [
    { type: "strip",
      instances: "$height",
      numPoints: "$width",
      spacing: "$spacing",
      y: "$instance*$spacing"
    }
  ]
}